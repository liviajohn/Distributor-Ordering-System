<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.distributororderingsystem.InventoryViewController">
   <children>
      <Text layoutX="462.0" layoutY="30.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Admin">
         <font>
            <Font name="Open Sans Medium" size="14.0" />
         </font>
      </Text>
      <ListView layoutX="91.0" layoutY="70.0" prefHeight="279.0" prefWidth="426.0" />
      <Button alignment="CENTER" contentDisplay="CENTER" onAction="#onLogoutButtonClick" style="-fx-background-color: #d20000; -fx-text-fill: white;" text="Logout" textAlignment="CENTER" translateX="525.0" translateY="10.0" />
      <Button fx:id="addToCart" layoutX="469.0" layoutY="320.0" mnemonicParsing="false" text="Add 1 to Cart">
         <font>
            <Font name="Open Sans SemiBold" size="16.0" />
         </font>
      </Button>
      <Button fx:id="checkout" layoutX="497.0" layoutY="361.0" mnemonicParsing="false" onMouseClicked="#onCheckoutButtonClick" text="Checkout">
         <font>
            <Font name="Open Sans SemiBold" size="16.0" />
         </font>
      </Button>
      <Button fx:id="back" layoutX="14.0" layoutY="19.0" mnemonicParsing="false" onAction="#onBackButtonClick" text="Back">
         <font>
            <Font name="Open Sans SemiBold" size="16.0" />
         </font>
      </Button>
      <ImageView fitHeight="49.0" fitWidth="197.0" layoutX="14.0" layoutY="341.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../java/org/example/distributororderingsystem/AcmeLogo.png" />
         </image>
      </ImageView>
      <Text fill="#313c73" layoutX="91.0" layoutY="57.0" text="Select Product:">
         <font>
            <Font name="Arial Bold" size="16" />
         </font>
      </Text>
   </children>
</AnchorPane>
